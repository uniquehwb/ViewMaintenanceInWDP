<?xml version="1.0" encoding="UTF-8"?>

<evaluationconfig>
        
      <updateProcessTimeout>1800000</updateProcessTimeout>
    
      <viewManager>
		<killDelay>10000</killDelay>  
      </viewManager>

      <regionServer>
          <overallTimeout>300000</overallTimeout>
		  <updatesTimeout>2000</updatesTimeout> 
		  <readWALDelay>10000</readWALDelay>  
      </regionServer>

      <client>
	    <fillTablesTimeout>1800000</fillTablesTimeout>
 		<createControlTables>true</createControlTables>            
      	<recreateTables>true</recreateTables>
      </client>
      
      <evaluationSet>	  
	    <evaluationParams>
		    <iterations>1</iterations>    
		    <variableParam>
		        <name>numOfViewManagers</name>
			    <startValue>2</startValue>
			    <endValue>10</endValue>
		    </variableParam>  
		    <variableParam>
		        <name>numOfClients</name>
			    <startValue>2</startValue>
			    <endValue>10</endValue>
		    </variableParam>   
	    </evaluationParams>
		<experiment>
		    <numOfClients>2</numOfClients>
		    <numOfRegionServers>2</numOfRegionServers>
		    <numOfViewManagers>2</numOfViewManagers>
		    <numOfKilledViewManagers>0</numOfKilledViewManagers>
		    <createtables>
			    <basetable>
					<name>bt1</name>
					<numOfRegions>5</numOfRegions>
			        <numOfOperations>101</numOfOperations>
				    <distribution>uniform</distribution>
					<useUpdates>false</useUpdates>
				    <useDeletes>false</useDeletes>
			    </basetable>
			    <deltaView>
		             <name>delta1</name>
		             <type>delta</type>
		             <basetable>bt1</basetable> 
		             <columns>
		                 <column>colAggKey</column>
		                 <column>colAggVal</column>
		   			</columns>
			         <numOfRegions>2</numOfRegions>
		         </deltaView>
			    <basetable>
					<name>bt2</name>
					<numOfRegions>5</numOfRegions>
			        <numOfOperations>100</numOfOperations>
				    <distribution>uniform</distribution>
					<useUpdates>false</useUpdates>
				    <useDeletes>false</useDeletes>
			    </basetable>
		         <deltaView>
		             <name>delta2</name>
		             <type>delta</type>
		             <basetable>bt2</basetable> 
		             <columns>
		                 <column>colAggKey</column>
		                 <column>colAggVal</column>
		   			</columns>
			         <numOfRegions>2</numOfRegions>
		         </deltaView>
		         <aggregationView>
		             <name>count1</name>
		             <type>count</type>
		             <basetable>delta1</basetable> 
		             <aggregationKey>colAggKey</aggregationKey>
		             <aggregationValue>colAggVal</aggregationValue>
			         <numOfRegions>2</numOfRegions>
			         <controlTable>bt1</controlTable>
		         </aggregationView>
		         <aggregationView>
		             <name>sum1</name>
		             <type>sum</type>
		             <basetable>delta1</basetable> 
		             <aggregationKey>colAggKey</aggregationKey>
		             <aggregationValue>colAggVal</aggregationValue>
			         <numOfRegions>2</numOfRegions>
			         <controlTable>bt1</controlTable>
		         </aggregationView>
		         <aggregationView>
		             <name>sum2</name>
		             <type>sum</type>
		             <basetable>delta1</basetable> 
		             <aggregationKey>colAggKey</aggregationKey>
		             <aggregationValue>colAggVal</aggregationValue>
			         <numOfRegions>2</numOfRegions>
			         <controlTable>bt1</controlTable>
		         </aggregationView>
			         <aggregationView>
			             <name>min1</name>
			             <type>min</type>
			             <basetable>delta1</basetable> 
			             <aggregationKey>colAggKey</aggregationKey>
			             <aggregationValue>colAggVal</aggregationValue>
				         <numOfRegions>2</numOfRegions>
				         <controlTable>bt1</controlTable>
			         </aggregationView>
			         <aggregationView>
			             <name>max1</name>
			             <type>max</type>
			             <basetable>delta1</basetable> 
			             <aggregationKey>colAggKey</aggregationKey>
			             <aggregationValue>colAggVal</aggregationValue>
				         <numOfRegions>2</numOfRegions>
				         <useIndex>true</useIndex>
				         <controlTable>bt1</controlTable>
			         </aggregationView>
			         <selectionView>
			             <name>selection1</name>
			             <basetable>delta1</basetable> 
			             <selectionKey>colAggVal</selectionKey>
			             <selectionOperation>&gt;</selectionOperation>
			             <selectionValue>10</selectionValue>
				         <numOfRegions>2</numOfRegions>
				         <controlTable>bt1</controlTable>
			         </selectionView>
		         <indexView>
		             <name>index1</name>
		             <basetable>delta1</basetable> 
		             <indexKey>colAggKey</indexKey>
		             <numOfRegions>2</numOfRegions>
		             <controlTable>bt1</controlTable>
		         </indexView>
		         <reverseJoinView>
		             <name>joinpair1</name>
		             <joinTables>
		                 <joinTable>
		                     <tableName>delta1</tableName>
		                     <tableKey>colAggKey</tableKey>
		                 </joinTable> 
		                 <joinTable>    
		                     <tableName>delta2</tableName>
		                     <tableKey>colAggKey</tableKey>
		                 </joinTable> 
		                 <joinTable>
		                     <tableName>delta3</tableName>
		                     <tableKey>colAggKey</tableKey>
		                 </joinTable>    
		             </joinTables>
			         <numOfRegions>10</numOfRegions>
			         <controlTable>true</controlTable>
		         </reverseJoinView>
		         <joinView>
		             <name>join1</name>
					 <joinPairView>joinpair1</joinPairView>
					 <compositeKey>
					     <colFam>delta1fam1</colFam>
					     <colFam>delta2fam1</colFam>
					 </compositeKey>
					 <numOfRegions>2</numOfRegions>
		         </joinView>      	               
		    </createtables>
		</experiment>         
	</evaluationSet>	
